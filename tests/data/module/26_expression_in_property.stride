use DesktopAudio version 1.0

import Generators

module Gen {
	ports: [
        mainOutputPort OutputPort {
            block: Output
        }
        propertyInputPort PropertyPort {
            name: "prop"
            block: Property
            default: 1.0
        }
    ]
    blocks: [
    ]
    streams: [
        Property + 0.1 >> Output;
    ]
}

signal Signal {
	default: 0.15
	domain: AudioDomain
}

Gen(prop: AudioIn[0] + 0.4) >> AudioOut[0];
Gen(prop: AudioIn[0] + AudioIn[1] + Signal ) >> AudioOut[1];
