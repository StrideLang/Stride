use DesktopAudio version 1.0

module Test {
	ports: [
		mainOutputPort OutputPort {
			block: Output
		}
		mainInputPort InputPort {
			block: Input
		}
		propertyInputPort TestPort {
			name: "test"
			block: TestBlock
		}
	]
	streams: [
		Input * TestBlock >> Output;
		]
}

AudioIn[0] >> Test(test: 0.5) >> AudioOut[0];
AudioIn[1] >> Test(test: 0.25) >> AudioOut[1];
