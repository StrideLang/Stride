use DesktopAudio version 1.0

module Test {
	ports: [
		mainInputPort InputPort {
			block: Input	
		}	
		mainOutputPort OutputPort {
			block: Output	
		}
	]
	blocks: [
		signal Dummy {default: 0.5}
		reaction Process {
			streams: [
				on >> ProcessInner();
			]
		}
		reaction ProcessInner {
			streams: [
				on >> ProcessInner2();
			]
		}
		reaction ProcessInner2 {
			streams: [
				Input * Dummy >> Output;
			]
		}
	]
	streams: [
		on >> Process();
	]
}

AudioIn[0] >> Test() >> AudioOut[0];